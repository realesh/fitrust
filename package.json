{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "check-format": "prettier --check \"src/**/*.{ts,tsx}\"",
    "lint": "tslint -p .",
    "typecheck": "tsc --noEmit",
    "test-src": "jest --coverage",
    "test-watch": "yarn test-src --watch",
    "test": "yarn lint && yarn typecheck && yarn check-format && yarn test-src"
  },
  "dependencies": {
    "expo": "^32.0.0",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-native-countdown-component": "^2.2.0",
    "react-native-snap-carousel": "^3.7.5",
    "react-native-svg-charts": "^5.2.0",
    "react-navigation": "^3.3.2"
  },
  "devDependencies": {
    "@types/expo": "^32.0.5",
    "@types/expo__vector-icons": "^9.0.1",
    "@types/jest": "^24.0.9",
    "@types/react": "^16.8.6",
    "@types/react-native": "^0.57.38",
    "@types/react-native-snap-carousel": "^3.7.1",
    "@types/react-native-svg-charts": "^5.0.1",
    "@types/react-navigation": "^3.0.4",
    "@types/react-test-renderer": "^16.8.1",
    "jest": "^24.1.0",
    "jest-expo": "^32.0.0",
    "prettier": "^1.16.4",
    "prettier-tslint": "^0.4.2",
    "react-native-testing-library": "^1.6.0",
    "react-test-renderer": "^16.8.3",
    "ts-jest": "^24.0.0",
    "tsconfig-kodefox": "^1.0.1",
    "tslint": "^5.13.1",
    "tslint-config-kodefox": "^1.1.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333"
  },
  "private": true,
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "ios.ts",
      "ios.tsx",
      "android.ts",
      "android.tsx"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "jsx": "react"
        }
      }
    }
  }
}
